    plot(0,xlim=xlim,ylim=ylim,cex=0,ylab=ylab,xlab="Position")
    for (y in 1:length(Ynames)){lines(dataframe[,Xname], dataframe[,Ynames[y]], type = "l", col = cols[y])}
    legend(xlim[2]/2, ylim[2], legend = Ynames, lty = 1, col = cols, bty = "n", ncol = 2)
    }
